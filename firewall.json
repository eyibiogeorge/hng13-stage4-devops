{
  "subnets": {
    "ns-public": {
      "ingress": [
        {"port": 80, "protocol": "tcp", "action": "allow"},
        {"port": 22, "protocol": "tcp", "action": "allow"}
      ],
      "egress": [
        {"port": 0, "protocol": "all", "action": "allow"}
      ]
    },
    "ns-private": {
      "ingress": [
        {"port": 80, "protocol": "tcp", "action": "deny"},
        {"port": 22, "protocol": "tcp", "action": "allow"}
      ],
      "egress": [
        {"port": 0, "protocol": "all", "action": "deny"}
      ]
    }
  }
}
